<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="api.connector.document.doctrine" class="Api\Sdk\Document\Connector\Doctrine\DocumentDoctrineConnector" parent="api.abstract_connector.doctrine" public="false" />
        <service id="api.connector.documentType.doctrine" class="Api\Sdk\Document\Connector\Doctrine\DocumentTypeDoctrineConnector" parent="api.abstract_connector.doctrine" public="false" />
        <service id="api.connector.document.data" class="Api\Sdk\Document\Connector\Data\DocumentDataConnector" parent="api.abstract_connector.data" public="false" />

        <service id="api.connector.document" class="Api\Sdk\Document\Connector\DocumentConnector" parent="api.abstract_connector" public="false">
            <argument type="collection">
                <argument type="service" id="api.connector.document.doctrine"></argument>
                <argument type="service" id="api.connector.documentType.doctrine"></argument>
                <argument type="service" id="api.connector.document.data"></argument>
            </argument>
        </service>
    </services>
</container>

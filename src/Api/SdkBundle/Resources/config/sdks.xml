<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="api.sdk.document" class="Api\Sdk\Document\DocumentSdk" public="false">
            <argument type="service" id="api.connector.document"></argument>
            <argument type="service" id="event_dispatcher"></argument>
        </service>
        <service id="api.sdk.document_type" class="Api\Sdk\Document\DocumentTypeSdk" public="false">
            <argument type="service" id="api.connector.document"></argument>
            <argument type="service" id="event_dispatcher"></argument>
        </service>
        <service id="api.sdk.user" class="Api\Sdk\User\UserSdk" public="false">
            <argument type="service" id="api.connector.user"></argument>
            <argument type="service" id="event_dispatcher"></argument>
        </service>
        <service id="api.sdk.role" class="Api\Sdk\Role\RoleSdk" public="false">
            <argument type="service" id="api.connector.role"></argument>
            <argument type="service" id="event_dispatcher"></argument>
        </service>
    </services>
</container>

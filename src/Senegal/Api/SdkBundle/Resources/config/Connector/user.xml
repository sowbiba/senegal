<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="pfd.connector.user.doctrine" class="Pfd\Sdk\User\Connector\Doctrine\UserDoctrineConnector" parent="pfd.abstract_connector.doctrine" public="false" />
        <service id="pfd.connector.user.propel" class="Pfd\Sdk\User\Connector\Propel\UserPropelConnector" parent="pfd.abstract_connector.propel" public="false" />
        <service id="pfd.connector.user.data" class="Pfd\Sdk\User\Connector\Data\UserDataConnector" parent="pfd.abstract_connector.data" public="false" />

        <service id="pfd.connector.user" class="Pfd\Sdk\User\Connector\UserConnector" parent="pfd.abstract_connector" public="false">
            <argument type="collection">
                <argument type="service" id="pfd.connector.user.doctrine"></argument>
                <argument type="service" id="pfd.connector.user.propel"></argument>
                <argument type="service" id="pfd.connector.user.data"></argument>
            </argument>
        </service>
    </services>
</container>

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="pfd.connector.market.webservice" class="Pfd\Sdk\Market\Connector\Webservice\MarketWebserviceConnector" public="false" >
            <argument type="service" id="pfd.client.espri"></argument>
        </service>

        <service id="pfd.connector.market" class="Pfd\Sdk\Market\Connector\MarketConnector" parent="pfd.abstract_connector" public="false">
            <argument type="collection">
                <argument type="service" id="pfd.connector.market.webservice"></argument>
            </argument>
        </service>
    </services>
</container>
